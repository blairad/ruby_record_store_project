<h1>Artists</h1>

<table>
  <tr>
    <th>Artist</th>

  </tr>
  <% for artist in @artists %>

  <tr>
    <td><a href="/artist/<%= artist.id %>"> <%= artist.name %></a></td>
  </tr>
  <% end %>
</table>
<br>
<br>

<form action="/artist/new">
  <input type="submit" value="Add Artist">
</form>

<br>

<form action="/artist/edit<%= artist.id %>">
  <input type="submit" value="Edit Artist">
</form>
